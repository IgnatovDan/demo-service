  "functions": [
    {
      "name": "createdOn",
      "function": {
        "logic": {
          "variables": [
            {
              "name": "createdOn",
              "parameter": true
            },
            {
              "name": "h",
              "parameter": false
            },
            {
              "name": "d",
              "parameter": false
            },
            {
              "name": "m",
              "parameter": false
            },
            {
              "name": "y",
              "parameter": false
            }
          ],
          "calls": [
            {
              "_type": "SetValue",
              "leftExpr": "h",
              "valueExpr": "Math.floor((new Date() - createdOn) / 3.6e6)"
            },
            {
              "_type": "SetValue",
              "leftExpr": "d",
              "valueExpr": "Math.floor(h / 24)"
            },
            {
              "_type": "SetValue",
              "leftExpr": "m",
              "valueExpr": "Math.floor(d / 30)"
            },
            {
              "_type": "SetValue",
              "leftExpr": "y",
              "valueExpr": "Math.floor(m / 12)"
            },
            {
              "_type": "Event",
              "flow": 0,
              "returnExpr": "(h > 24 ? d > 30 ? m > 12 ? y + ' years' : m + ' months' : d + ' days' : h + ' hours')"
            }
          ]
        }
      }
    },
    {
      "name": "handleError",
      "function": {
        "logic": {
          "variables": [
            {
              "name": "error",
              "type": "object",
              "parameter": true
            },
            {
              "name": "errorMessage",
              "value": "",
              "type": "string",
              "parameter": false
            },
            {
              "name": "errorTitle",
              "value": "",
              "type": "string",
              "parameter": false
            }
          ],
          "calls": [
            {
              "_type": "Switch",
              "expr": "!!error.message || !!error.__details || !!(error.response && (error.response.ExceptionMessage || error.response.MessageDetail))",
              "cases": [
                {
                  "valueExpr": "true",
                  "calls": [
                    {
                      "_type": "SetValue",
                      "leftExpr": "errorMessage",
                      "valueExpr": "error.message || error.__details || (error.response && (error.response.ExceptionMessage || error.response.MessageDetail))"
                    },
                    {
                      "_type": "SetValue",
                      "leftExpr": "errorTitle",
                      "valueExpr": "error.response && error.response.Message || 'Error'"
                    }
                  ]
                }
              ],
              "otherwise": [
                {
                  "_type": "Switch",
                  "expr": "!!error.response && !!error.response.ModelState",
                  "cases": [
                    {
                      "valueExpr": "true",
                      "calls": [
                        {
                          "_type": "Loop",
                          "valueExpr": "error.response.ModelState",
                          "calls": [
                            {
                              "_type": "SetValue",
                              "leftExpr": "errorMessage",
                              "valueExpr": "errorMessage + loopValue + '<br/>'"
                            }
                          ],
                          "indexName": "loopIndex",
                          "valueName": "loopValue"
                        },
                        {
                          "_type": "SetValue",
                          "leftExpr": "errorTitle",
                          "valueExpr": "error.response.Message"
                        }
                      ]
                    }
                  ],
                  "otherwise": [
                    {
                      "_type": "SetValue",
                      "leftExpr": "errorMessage",
                      "valueExpr": "error.response && error.response.Message || 'Unknown error'"
                    },
                    {
                      "_type": "SetValue",
                      "leftExpr": "errorTitle",
                      "valueExpr": "'Error'"
                    }
                  ]
                }
              ]
            },
            {
              "_type": "ShowAlert",
              "title": "=errorTitle",
              "message": "=errorMessage"
            }
          ]
        }
      }
    }
